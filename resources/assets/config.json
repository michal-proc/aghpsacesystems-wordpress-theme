{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "__comment:": "It's important to remember about changing 1st part of \"publicPath\". PRODUCTION: /wp-content/themes/aghspacesystems",
  "publicPath": "/wp-content/themes/aghspacesystems",
  "devUrl": "http://localhost/aghspacesystems-site",
  "proxyUrl": "http://localhost:3000/aghspacesystems-site",
  "cacheBusting": "[name]_[contenthash]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php"
  ]
}
